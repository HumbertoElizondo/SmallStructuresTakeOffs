@model SmallStructuresTakeOffs.Models.SmallStructure

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>SmallStructure</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="SmStId" />
            <div class="form-group">
                <label asp-for="ThisStructure" class="control-label"></label>
                <select asp-for="ThisStructure" class="form-control" asp-items="@ViewBag.HWList"></select>

                @*<input asp-for="ThisStructure" class="form-control" />*@
                <span asp-validation-for="ThisStructure" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="HWcode" class="control-label"></label>
                <input asp-for="HWcode" class="form-control" />
                <span asp-validation-for="HWcode" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
